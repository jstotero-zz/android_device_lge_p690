	mkdir /data/wpstiles/ 0777 shell
	mkdir /etc/wiper 0775 location qcom_oncrpc

## ERS
    mkdir /data/data/com.lge.ers
    mkdir /data/data/com.lge.ers/android
    mkdir /data/data/com.lge.ers/kernel
    mkdir /data/data/com.lge.ers/arm9
